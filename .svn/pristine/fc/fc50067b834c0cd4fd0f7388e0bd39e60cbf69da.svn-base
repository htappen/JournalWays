.map-viewport{ position:relative; width:400px; height:300px; border:1px solid black; overflow:hidden; margin:0 0 20px 0; background: url(/eajaxupload/images/noPic.png); }
/*.map-viewport img { margin-top: -20px; }*/
.level{ position:absolute; left:0; top:0; z-index:10;}
.current-level{ z-index:20; }
#slider { margin-left:10px; margin-bottom:10px; clear:both; }
#map{ width:1200px; height:900px; position:relative; left:0; top:0; cursor:pointer ;}
img{ border:0; }
.block{ float:left; margin-right:2%; margin-top:10px; margin-left:20px; }
#page-wrap { position:relative; overflow:hidden; width:900px; }
#preview { position:relative; overflow:hidden; float:left; margin-left: 100px; margin-top:10px; width: 430px; height:auto; }
#preview .imgpreview { float:left; width: 180px; height: 150px; margin: 0 0 10px 20px; position:relative; overflow:hidden; }
#newImagePreview { position:relative; overflow:hidden; width:auto; height:auto; }
 #cropPreview img{ border: solid 1px; height: 300px; width: 400px; display: none; }